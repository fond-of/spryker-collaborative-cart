{
  "name": "fond-of-spryker/collaborative-cart",
  "description": "Collaborative cart module",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    }
  ],
  "require": {
    "php": ">=8.0",
    "fond-of-impala/company-users-rest-api": "^1.0.0",
    "fond-of-spryker/company-user-reference": "dev-master",
    "fond-of-spryker/company-user-reference-quote-connector": "dev-master",
    "spryker/customer": "^7.0.0",
    "spryker/company-user": "^2.0.0",
    "spryker/quote": "^2.0.0",
    "spryker/quote-extension": "^1.0.0",
    "spryker/permission": "^1.0.0",
    "spryker/permission-extension": "^1.0.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "^1.0",
    "spryker/code-sniffer": "^0.12",
    "spryker-sdk/phpstan-spryker": "^0.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "src/Generated/"
    }
  }
}
